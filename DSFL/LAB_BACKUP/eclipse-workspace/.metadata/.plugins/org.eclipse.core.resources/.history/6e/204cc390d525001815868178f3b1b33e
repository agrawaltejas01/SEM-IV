#include "graph.h"
#include<iostream>
#include<string>
using namespace std;

void graph:: getdata()
{
	cout<<"Enter number of nodes : ";
	cin>>n;
	cout<<endl<<endl;

	for(int i=0;i<n;i++)
	{
		for(int j=0;j<n;j++)
		{
			cout<<"Enter edge between "<<i<<" and "<<j<<" : ";
			cin>>G[i][j];
		}
	}


}

void graph:: print_adj_matrix()
{
	for(int i=0;i<n;i++)
	{
		for(int j=0;j<n;j++)
			cout<<G[i][j]<<" ";

		cout<<endl;
	}
}

