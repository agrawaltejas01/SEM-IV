#include <iostream>
#include<string>
#include "stack.h"
#include "inpost.h"
using namespace std;

int main()
{
	stack s;
	inpost post;
	/*cout<<"Before Pushing\n";
cout<<"Top : "<<s.tope()<<"\n";
	cout<<"Empty? :"<<s.isempty()<<endl;
	cout<<"Full? :"<<s.isfull()<<endl;

	s.push(1);
	s.push(2);
	s.push(3);

	cout<<"After Pushing\n";

	cout<<"Empty? :"<<s.isempty()<<"\n";
	cout<<"Full? :"<<s.isfull()<<"\n";

	cout<<"Top : "<<s.tope()<<"\n";

	cout<<"Pop :"<<s.pop()<<endl;

	coucout<<"Top : "<<s.tope()<<"\n";t<<"Top : "<<s.tope()<<"\n";

	return 0;*/

	string arr;
	string ans;

	cout<<"Enter String :";
	cin>>arr;
	cout<<endl;

	int len=arr.length(),k=0;

	for(int i=0;i<len;i++)
	{
		char x=arr[i];

		if( (x>='A' && x<='Z') || (x>='a' && x<='z') )
		{
			//ans[k++]=arr[i];
			cout<<""<<x;
		}

		else if(x=='(')
					s.push(x);

		else if(post.isoperator(x))
		{
			while( post.priority(s.tope()) >= post.priority(x) )
			{
				//ans[k++]=s.pop();
				cout<<""<<s.pop();

			}

			s.push(x);
		}

		else if(x==')')
		{
			while(s.tope() != '(')
			{
				//ans[k++]=s.pop();
				cout<<""<<s.pop();
			}

		}

		else if(x=='(')
			s.push(x);

		if(i==len-1 && !s.isempty())
		{
			while(!s.isempty())
				cout<<""<<s.pop();//ans[k++]=s.pop();
		}
	}

	while(!s.isempty())
			{
					cout<<""<<s.pop();//ans[k++]=s.pop();
			}

	/*for(int i=0;i<len;i++)
		cout<<""<<ans[i];*/

	cout<<endl;
}
