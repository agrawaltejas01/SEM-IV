#ifndef QUEUES_H_
#define QUEUES_H_

#include<iostream>
#include<malloc.h>
#include<string>
using namespace std;

struct patient
{
	string name;
	string add;
	string ph_no;
};

struct node
{
	patient data;
	int pri;

	node *link;
};

class queues
{
	node *front,*rear;

	int max=50,id;

	public:
		queues();
		int isfull();
		int isempty();

		void enqueue(string n,string ad,string p_n);
		void dequeue();


};

#endif /* QUEUES_H_ */
