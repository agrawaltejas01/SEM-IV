#include "inpost.h"

int inpost ::  isoperator(char x)
{
	if(x=='(' || x=='/' || x=='*' || x=='+' || x=='-' || x=='^')
		return 1;

	else
		return 0;
}

int inpost ::  priority(char x)
{
	if(x=='^')
		return 4;

	else if(x=='/' || x=='*')
		return 3;

	else if(x=='+' || x=='-')
		return 2;

	else if(x=='(')
		return 1;

}

