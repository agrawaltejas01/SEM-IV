#ifndef QUEUES_H_
#define QUEUES_H_

struct node
{
	int data;
	int pri;

	node *link;
};

class queues
{
	node *front,*rear;

	public:
		queues();
		int isfull();
		int isempty();

		void getdata();

		void enqueue();
		void dequeue();


};

#endif /* QUEUES_H_ */
