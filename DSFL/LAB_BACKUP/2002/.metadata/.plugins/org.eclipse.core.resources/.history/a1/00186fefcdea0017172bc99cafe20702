#include <iostream>
#include "stack.h"
using namespace std;

int main()
{
	stack s;

	cout<<"Before Pushing\n";

	cout<<"Empty? :"<<s.isempty()<"\n";
	cout<<"Full? :"<<s.isfull()<"\n";

	s.push(1);
	s.push(2);
	s.push(3);

	cout<<"After Pushing\n"

	cout<<"Empty? :"<<s.isempty()<"\n";
	cout<<"Full? :"<<s.isfull()<"\n";

	cout<<"Top : "<<s.tope()<<"\n";

	cout<<"Pop :"<<s.pop

	return 0;
}
