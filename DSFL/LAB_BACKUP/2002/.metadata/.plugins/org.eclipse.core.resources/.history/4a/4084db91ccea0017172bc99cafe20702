#ifndef STACK_H_
#define STACK_H_

typedef struct node
{
		int data;
		struct node *link;
}node;

class stack
{
	node *Top;

public:
	stack();

	int isempty();
	int isfull();

	void push(int x);
	int tope();
	int pop();

};

#endif /* STACK_H_ */
